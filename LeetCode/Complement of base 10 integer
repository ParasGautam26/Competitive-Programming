class Solution {
    public int complement(int n) 
    { 
        int number_of_bits =  
               (int)(Math.floor(Math.log(n) / 
                             Math.log(2))) + 1; 
        return ((1 << number_of_bits) - 1) ^ n; 
    } 
    
    public int bitwiseComplement(int n) {
        int x= complement(n);
        return x;
    }
}
